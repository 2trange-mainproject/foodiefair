<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.multicampus.foodiefair.dao.QuestionDAO">
    <insert id="insertQuestion" parameterType="com.multicampus.foodiefair.dto.QuestionDTO">
        INSERT INTO Question (questionType, questionDate, questionContent)
        VALUES (#{questionType}, #{questionDate}, #{questionContent})
    </insert>
</mapper>
